<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox fx:id="pageContents" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.guilford.StartPageController">
   <children>
      <VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="1280.0" spacing="20.0">
         <children>
            <Pane fx:id="ribbon1" prefHeight="100.0" prefWidth="1280.0" />
            <Label fx:id="title" stylesheets="@primer-dark.css" text="NFL Statistics">
               <font>
                  <Font name="Sinhala MN Bold" size="96.0" />
               </font>
            </Label>
            <Button fx:id="startButton" onAction="#switchToSecondary" stylesheets="@primer-dark.css" text="Start App" textFill="#48ff91">
               <font>
                  <Font name="Sinhala Sangam MN" size="34.0" />
               </font>
            </Button>
            <Button fx:id="quitButton" mnemonicParsing="false" onAction="#quit" text="Quit App" textFill="#ff5454">
               <font>
                  <Font name="Sinhala Sangam MN" size="34.0" />
               </font>
               <padding>
                  <Insets bottom="50.0" />
               </padding>
            </Button>
            <Pane fx:id="ribbon2" prefHeight="100.0" prefWidth="1280.0" />
         </children>
      </VBox>
   </children>
</VBox>
